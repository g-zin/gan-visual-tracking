{
  "use_gpu": true,
  "init_model_path": "./models/imagenet-vgg-m.mat",
  "model_path": "./models/rt_mdnet_vot.pth",
  "batch_frames": 8,
  "batch_pos": 64,
  "batch_neg": 196,
  "overlap_pos": [
    0.7,
    1
  ],
  "overlap_neg": [
    0,
    0.5
  ],
  "img_size": 107,
  "lr": 0.0001,
  "w_decay": 0.0005,
  "momentum": 0.9,
  "grad_clip": 10,
  "ft_layers": [
    "conv",
    "fc"
  ],
  "lr_mult": {
    "fc": 1
  },
  "n_cycles": 1000,
  "padding": 1.2,
  "padding_ratio": 5.0,
  "frame_interval": 2
}